te-io/date-fns'\r\nimport UpdateIcon from '@material-ui/icons/Update'\r\n//connecting to redux store\r\nimport { connect } from 'react-redux'\r\n//importing actions\r\nimport { updateNote } from '../../redux/note/note-actions'\r\n\r\nconst useStyles = makeStyles((theme) => ({\r\n  root: {\r\n    marginLeft: theme.spacing(4),\r\n    position: 'absolute',\r\n    right: '18px',\r\n    bottom: '-18px',\r\n    color: '#fff',\r\n  },\r\n}))\r\n\r\nconst EditForm = ({ noteToEdit, updateNote }) => {\r\n  const [note, setNote] = useState(noteToEdit)\r\n  const classes = useStyles()\r\n\r\n  const handleChange = (e) => {\r\n    const { name, value } = e.target\r\n    setNote({ ...note, [name]: value })\r\n  }\r\n\r\n  const convertToDefEventPara = (name, value) => ({\r\n    target: {\r\n      name,\r\n      value,\r\n    },\r\n  })\r\n\r\n  const handleSubmit = (e) => {\r\n    e.preventDefault()\r\n    updateNote(note)\r\n  }\r\n\r\n  return (\r\n    <div>\r\n      <form>\r\n        <input\r\n          name=\"title\"\r\n          onChange={handleChange}\r\n          placeholder=\"Title\"\r\n          value={note.title}\r\n        />\r\n        <textarea\r\n          name=\"body\"\r\n          onChange={handleChange}\r\n          placeholder=\"Take a note...\"\r\n          rows=\"3\"\r\n          value={note.body}\r\n        />\r\n\r\n        <MuiPickersUtilsProvider utils={DateFnsUtils}>\r\n          <DatePicker\r\n            disableFuture\r\n            openTo=\"year\"\r\n            format=\"dd/MM/yyyy\"\r\n            label=\"Date of birth\"\r\n            views={['year', 'month', 'date']}\r\n            name=\"date\"\r\n            value={note.date}\r\n            onChange={(date) =>\r\n              handleChange(convertToDefEventPara('date', date))\r\n            }\r\n          />\r\n        </MuiPickersUtilsProvider>\r\n\r\n        <Fab\r\n          onClick={handleSubmit}\r\n          size=\"small\"\r\n          className={classes.root}\r\n          color=\"primary\"\r\n        >\r\n          <UpdateIcon />\r\n        </Fab>\r\n      </form>\r\n    </div>\r\n  )\r\n}\r\n\r\nconst mapStateToProps = (state) => {\r\n  return {\r\n    noteToEdit: state.notes.noteToEdit,\r\n  }\r\n}\r\n\r\nconst mapDispatchToProps = (dispatch) => {\r\n  return {\r\n    updateNote: (note) => dispatch(updateNote(note)),\r\n  }\r\n}\r\n\r\nexport default connect(mapStateToProps, mapDispatchToProps)(EditForm)\r\n","C:\\Users\\TANAY\\Desktop\\react\\assignment-note-app\\src\\index.js",[],"C:\\Users\\TANAY\\Desktop\\react\\assignment-note-app\\src\\reportWebVitals.js",[],"C:\\Users\\TANAY\\Desktop\\react\\assignment-note-app\\src\\App.js",[],"C:\\Users\\TANAY\\Desktop\\react\\assignment-note-app\\src\\redux\\store.js",[],"C:\\Users\\TANAY\\Desktop\\react\\assignment-note-app\\src\\components\\NoteList.js",[],"C:\\Users\\TANAY\\Desktop\\react\\assignment-note-app\\src\\redux\\root-reducer.js",[],"C:\\Users\\TANAY\\Desktop\\react\\assignment-note-app\\src\\components\\TextArea\\TextArea.js",["105"],"C:\\Users\\TANAY\\Desktop\\react\\assignment-note-app\\src\\components\\EditForm\\EditForm.js",["106"],"C:\\Users\\TANAY\\Desktop\\react\\assignment-note-app\\src\\components\\Note.js",[],"C:\\Users\\TANAY\\Desktop\\react\\assignment-note-app\\src\\redux\\note\\note-reducer.js",[],"C:\\Users\\TANAY\\Desktop\\react\\assignment-note-app\\src\\redux\\note\\note-actions.js",[],"C:\\Users\\TANAY\\Desktop\\react\\assignment-note-app\\src\\redux\\note\\note-types.js",[],{"ruleId":"107","replacedBy":"108"},{"ruleId":"109","replacedBy":"110"},{"ruleId":"111","severity":1,"message":"112","line":7,"column":3,"nodeType":"113","messageId":"114","endLine":7,"endColumn":21},{"ruleId":"111","severity":1,"message":"112","line":8,"column":3,"nodeType":"113","messageId":"114","endLine":8,"endColumn":21},{"ruleId":"111","severity":1,"message":"112","line":7,"column":3,"nodeType":"113","messageId":"114","endLine":7,"endColumn":21},{"ruleId":"111","severity":1,"message":"112","line":8,